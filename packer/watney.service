[Unit]
Description=Watney Service

[Service]
ExecStart=/usr/bin/python3 -u /home/USER_PLACEHOLDER/watney/server.py
WorkingDirectory=/home/USER_PLACEHOLDER/watney
StandardOutput=journal
StandardError=journal
Restart=always
TimeoutStopSec=5s
User=USER_PLACEHOLDER
Group=USER_PLACEHOLDER

[Install]
WantedBy=multi-user.target
